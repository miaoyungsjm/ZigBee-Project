    341          /***************************************************************************************************
    342           * @fn      MT_UartProcessZAppData
    343           *
    344           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    345           *          |  1  |  2   |       1         |  1   |
    346           *
    347           *          Parses the data and determine either is SPI or just simply serial data
    348           *          then send the data to correct place (MT or APP)
    349           *
    350           * @param   port    - UART port
    351           *          event   - Event that causes the callback
    352           *
    353           *
    354           * @return  None
    355           ***************************************************************************************************/
    356          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    357          {
    358          
    359            osal_event_hdr_t  *msg_ptr;
    360            uint16 length = 0;
    361            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    362          
    363            /*
    364               If maxZAppBufferLength is 0 or larger than current length
    365               the entire length of the current buffer is returned.
    366            */
    367            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    368            {
    369              length = MT_UartMaxZAppBufLen;
    370            }
    371            else
    372            {
    373              length = rxBufLen;
    374            }
    375          
    376            /* Verify events */
    377            if (event == HAL_UART_TX_FULL)
    378            {
    379              // Do something when TX if full
    380              return;
    381            }
    382          
    383            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    384            {
    385              if ( App_TaskID )
    386              {
    387                /*
    388                   If Application is ready to receive and there is something
    389                   in the Rx buffer then send it up
    390                */
    391                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    392                {
    393                  /* Disable App flow control until it processes the current data */
    394                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    395          
    396                  /* 2 more bytes are added, 1 for CMD type, other for length */
    397                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    398                  if ( msg_ptr )
    399                  {
    400                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    401                    msg_ptr->status = length;
    402          
    403                    /* Read the data of Rx buffer */
    404                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
